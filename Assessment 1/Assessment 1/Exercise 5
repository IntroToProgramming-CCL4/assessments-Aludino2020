calendar = {
'1':31, 
'2':28,
'3':31,
'4':30,
'5':31,
'6':30,
'7':31,
'8':31,
'9':30,
'10':31,
'11':30,
'12':31 
}
leap = input("Is there leap year? Y/N: ").upper()
if leap == "Y":
    calendar['2'] = 29

num = (input("Enter month number: "))
if num in calendar.keys():
    print(calendar[num])
else:
    print("Invalid input")




